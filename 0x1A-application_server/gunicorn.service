[Unit]
Description=Gunicorn instance to serve AirBnB_clone_v2
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/AirBnB_clone_v2
ExecStart=/home/ubuntu/AirBnB_clone_v2/venv/bin/gunicorn --config /home/ubuntu/AirBnB_clone_v2/gunicorn_config/gunicorn_config.py web_dynamic.2-hbnb:app

[Install]
WantedBy=multi-user.target
